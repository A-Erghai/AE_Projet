@model AE_Projet.Models.Presence

@{
    Layout = "_LayoutAdmin";
}



    <section class="categories-section spad">
        <div class="row">
            <div class="col-md-4">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="Matricule" class="control-label"></label>
                        <select asp-for="Matricule" class="form-control" asp-items="ViewBag.Matricule"></select>
                    </div>
                    <div class="form-group">
                        <label asp-for="@DateTime.Now " class="control-label"></label>
                        <input asp-for="@DateTime.Now" class="form-control" />
                        <span asp-validation-for="Date" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Id_Salle" class="control-label"></label>
                        <select asp-for="Id_Salle" class="form-control" asp-items="ViewBag.Id_Salle"></select>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Ajouter" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>

        <div>
            <a asp-action="IndexPre">Back to List</a>
        </div>

        </section>