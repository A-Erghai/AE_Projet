@model IEnumerable<AE_Projet.Models.Presence>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.etudiant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.salle)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.etudiant.Matricule)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.salle.Nom_Salle)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Presence">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Presence">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Presence">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
