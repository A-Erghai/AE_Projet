@model AE_Projet.Models.Seance




@{
    Layout = "_LayoutAdmin";
}
<section class="categories-section spad">
    <div class="container">
        <div class="section-title">
            <a class="dex-btn " style="background-color: #0039e6;color: white;">Supprimer </a>
        </div>
        <h3>Êtes-vous sûr de vouloir supprimer ?</h3>
        <div>
            <div>
                <hr />
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Heure_D)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Heure_D)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Heure_F)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Heure_F)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.filiere)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.filiere.Nom_Filiere)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.salle)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.salle.Nom_Salle)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.matiere)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.matiere.Nom_Matiere)
                    </dd>
                </dl>

                <form asp-action="SuppSea">
                    <input type="hidden" asp-for="Id_Seance" />
                    <input type="submit" value="Supprimer" class="btn btn-danger" /> |
                    <a asp-action="IndexSea" class="btn btn-success">Annuler</a>
                </form>
            </div>
        </div>
    </div>

</section>
