@model AE_Projet.Models.Filiere

@{
    Layout = "_LayoutAdmin";
}

<section class="categories-section spad">
    <div class="container">
        <div class="section-title">
            <h2>Filiere Salle</h2>
        </div>
        <form asp-action="AjtF">
            <div class="formulaire">
                <div class="form-group">
                    <label asp-for="Nom_Filiere" class="control-label"></label>
                    <input asp-for="Nom_Filiere" class="form-control" />
                    <span asp-validation-for="Nom_Filiere" class="text-danger"></span>
                </div>
                <div>
                    <a asp-action="IndexF" class="btn btn-danger">Annuler</a>
                    <input type="submit" value="Ajouter" class="btn btn-primary" />
                </div>
            </div>
        </form>
    </div>
</section>