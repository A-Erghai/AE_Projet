@model AE_Projet.Models.Filiere

@{
    Layout = "_LayoutAdmin";
}
    <section class="categories-section spad">
        <div class="container">
            <div class="section-title">
                <h2>Gestion Administration</h2>
                <a class="dex-btn " style="background-color: #0039e6;color: white;">Supprimer etudiant</a>
            </div>
            <h3>Êtes-vous sûr de vouloir supprimer La filiere ?</h3>
            <div>
                <div>
                    <h4>Etudiant</h4>
                    <hr />
                    <dl class="row">
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.Nom_Filiere)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Nom_Filiere)
                        </dd>
                    </dl>

                    <form asp-action="SuppF">
                        <input type="hidden" asp-for="Id_Filiere" />
                        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
                        <a asp-action="IndexF" class="btn btn-success">Annuler</a>
                    </form>
                </div>
            </div>
        </div>

    </section>